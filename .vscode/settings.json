{
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        ".devcontainer",
        ".github",
        "_config.yml",
        "Gemfile",
        "Gemfile.lock"
    ],
    "rubyLsp.enabledFeatures": {
        "documentHighlights": true,
        "documentLink": true,
        "documentSymbols": true,
        "foldingRanges": true,
        "selectionRanges": true,
        "semanticHighlighting": true,
        "formatting": true,
        "diagnostics": true,
        "codeActions": true,
        "onTypeFormatting": true
    },
}
